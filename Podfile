# Uncomment this line to define a global platform for your project
platform :ios, '10.0'

#project 'GenericKit.xcodeproj'

def install_pods
    pod 'Alamofire', '~> 4.8'
    pod 'Alamofire-SwiftyJSON', '~> 3'
    pod 'ObjectMapper', '~> 3.4'
    pod 'Kingfisher', '~> 5'
    pod 'SnapKit', '~> 4.2'
end

target 'GenericKitDemo' do
    install_pods
end
